<resources>
  <string name="pref_network_usage_log_settings_key" translatable="false">pref_network_usage_log_settings_key</string>
  <!-- Title of the network usage log preference in Private Compute Core settings. [CHAR_LIMIT=NONE] -->
  <string name="pref_network_usage_log_settings_title">Network usage log</string>
  <!-- Summary of the network usage log preference in Private Compute Core settings. [CHAR_LIMIT=NONE] -->
  <string name="pref_network_usage_log_settings_summary">View Private Compute Services network usage</string>

  <string name="pref_network_usage_log_enabled_key" translatable="false">pref_network_usage_log_enabled_key</string>
  <bool name="pref_network_usage_log_enabled_default">false</bool>
  <!-- Text displayed on the switch used to enable/disable the network usage log. [CHAR_LIMIT=NONE] -->
  <string name="pref_network_usage_log_enabled_title">Log network activity</string>

  <!-- Message describing the Network Usage Log feature in Private Compute Core, displayed under the title. Keep is being used in a generic context. [CHAR_LIMIT=NONE] -->
  <string name="network_usage_log_subtext">
    This keeps a log of all Private Compute Services network activity. The log does not leave your device, and only you can view it.
  </string>

  <!-- Title of the alert dialog shown when the user attempts to disable the network usage log. [CHAR_LIMIT=NONE] -->
  <string name="disable_network_usage_log_alert_title">Turn off network usage log</string>
  <!-- Message displayed in the alert dialog shown when the user attempts to disable the network usage log. [CHAR_LIMIT=NONE] -->
  <string name="disable_network_usage_log_alert_message">
    Stop logging Private Compute Services network activity.
    Turning off the log will also delete current network usage log data.
  </string>
  <!-- Text displayed on the button which dismisses the alert dialog shown when the user attempts to disable the network usage log. [CHAR_LIMIT=NONE] -->
  <string name="disable_network_usage_log_alert_button_cancel">Cancel</string>
  <!-- Text displayed on the button which turns off the network usage log in the alert dialog. [CHAR_LIMIT=NONE] -->
  <string name="disable_network_usage_log_alert_button_turn_off">Turn off</string>

</resources>
